<h1>Add a New Quote</h1>
<%= form_with model: @quote, local: true do |form| %>
  <div>
    <%= form.label :content, 'Quote' %><br>
    <%= form.text_area :content %>
  </div>
  <div>
    <%= form.submit 'Submit', class: 'btn' %>
  </div>
<% end %>

<%= link_to 'Back to Quotes', root_path, class: 'btn' %>
